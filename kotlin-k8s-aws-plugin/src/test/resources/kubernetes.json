  "apiVersion" : "apps/v1",
  "kind" : "Deployment",
  "metadata" : {
    "annotations" : {
      "app.quarkus.io/vcs-url" : "https://github.com/ElinaValieva/micronaut-quickstarts.git",
      "app.quarkus.io/build-timestamp" : "2020-06-06 - 12:40:57 +0000",
      "app.quarkus.io/commit-id" : "f4923713550d5529fb46d5cb023346da93faea1d"
    },
    "labels" : {
      "app.kubernetes.io/name" : "quarkus-quickstart",
      "app.kubernetes.io/version" : "latest",
      "app" : "quarkus"
    },
    "name" : "quarkus-quickstart"
  },
  "spec" : {
    "replicas" : 1,
    "selector" : {
      "matchLabels" : {
        "app.kubernetes.io/name" : "quarkus-quickstart",
        "app.kubernetes.io/version" : "latest"
      }
    },
    "template" : {
      "metadata" : {
        "annotations" : {
          "app.quarkus.io/vcs-url" : "https://github.com/ElinaValieva/micronaut-quickstarts.git",
          "app.quarkus.io/build-timestamp" : "2020-06-06 - 12:40:57 +0000",
          "app.quarkus.io/commit-id" : "f4923713550d5529fb46d5cb023346da93faea1d"
        },
        "labels" : {
          "app.kubernetes.io/name" : "quarkus-quickstart",
          "app.kubernetes.io/version" : "latest",
          "app" : "quarkus"
        }
      },
      "spec" : {
        "containers" : [ {
          "env" : [ {
            "name" : "KUBERNETES_NAMESPACE",
            "valueFrom" : {
              "fieldRef" : {
                "fieldPath" : "metadata.namespace"
              }
            }
          } ],
          "image" : "docker.io/elvaliev/quarkus-quickstart:latest",
          "imagePullPolicy" : "Always",
          "livenessProbe" : {
            "failureThreshold" : 3,
            "httpGet" : {
              "path" : "/health/live",
              "port" : 8090,
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 0,
            "periodSeconds" : 30,
            "successThreshold" : 1,
            "timeoutSeconds" : 10
          },
          "name" : "quarkus-quickstart",
          "ports" : [ {
            "containerPort" : 8090,
            "name" : "http",
            "protocol" : "TCP"
          } ],
          "readinessProbe" : {
            "failureThreshold" : 3,
            "httpGet" : {
              "path" : "/health/ready",
              "port" : 8090,
              "scheme" : "HTTP"
            },
            "initialDelaySeconds" : 0,
            "periodSeconds" : 30,
            "successThreshold" : 1,
            "timeoutSeconds" : 10
          }
        } ],
        "serviceAccount" : "quarkus-quickstart"
      }
    }
  }
}